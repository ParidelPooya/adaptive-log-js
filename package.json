    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "code": "^5.1.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "lab": "^14.3.1",
    "pre-commit": "^1.2.2",
    "webpack": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git:/github.com/ParidelPooya/execute-js"
  },
